{
  "name": "autograde-tcg",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build:client": "cd client && npm run build",
    "build:server": "tsc && cp src/db/schema.sql dist/db/schema.sql",
    "build": "npm run build:client && npm run build:server",
    "start": "node dist/index.js",
    "seed": "ts-node src/db/seed.ts",
    "ingest": "ts-node -e \"const { ingestPrices } = require('./src/services/priceIngestion'); console.log('Inserted:', ingestPrices())\"",
    "seed:user": "ts-node src/db/seedTestUser.ts",
    "seed:trades": "ts-node src/db/seedTradeUsers.ts",
    "seed:prices": "ts-node src/db/seedPrices.ts",
    "db:migrate": "ts-node src/db/migrate.ts",
    "db:backup": "ts-node src/db/backup.ts",
    "db:setup": "npm run seed && npm run seed:prices",
    "db:reset": "npm run db:backup && rm -f data/autograde.db && npm run db:setup",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18"
  },
  "type": "commonjs",
  "dependencies": {
    "@types/multer": "^2.0.0",
    "@types/string-similarity": "^4.0.2",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.6.2",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "string-similarity": "^4.0.4",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.3.0",
    "@types/uuid": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
